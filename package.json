{
  "name": "rescape-place",
  "version": "0.0.160",
  "description": "Apollo-backed place schema",
  "main": "lib/index.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-place.git"
  },
  "author": "calocan <andy@likuski.org>",
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib esm coverage",
    "lint": "eslint src",
    "test": "cross-env jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "apollo-client": "^2.6.8",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-error": "^1.1.12",
    "apollo-link-schema": "^1.2.4",
    "fetch": "^1.1.0",
    "folktale": "^2.3.2",
    "graphql-tag": "^2.10.1",
    "http-https": "^1.0.0",
    "inflected": "^2.0.4",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "ramda-lens": "^0.1.2",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.17",
    "apollo-test-utils": "^0.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv": "8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-wait": "^1.0.9",
    "eslint": "6.1.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "install-peers-cli": "^2.1.1",
    "jest": "24.9.0",
    "jest-cli": "^24.9.0",
    "jsdoc-babel": "^0.5.0",
    "jsdom": "^15.1.1",
    "localstorage-polyfill": "^1.0.1",
    "longjohn": "^0.2.12",
    "moment": "^2.24.0",
    "promise": "8.0.3",
    "react-dom": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "peerDependencies": {
    "graphql": ">=14.0.2",
    "react": ">=16.10.1",
    "react-dom": "^16.12.0",
    "rescape-apollo": ">=0.0.108",
    "rescape-geospatial-sankey": ">=0.7.17",
    "rescape-helpers": ">=1.1.99",
    "rescape-helpers-component": ">=1.5.38",
    "rescape-log": ">=0.1.11",
    "rescape-ramda": ">=1.5.78",
    "rescape-sample-data": ">=1.2.60",
    "rescape-validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
